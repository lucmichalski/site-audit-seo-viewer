{
  "name": "site-audit-seo-viewer",
  "version": "2.5.0",
  "description": "Display audit.json as table",
  "author": "Stanislav Popov <popstas@gmail.com>",
  "homepage": "https://github.com/viasite/site-audit-seo-viewer",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=3000 nuxt",
    "build": "nuxt build",
    "deploy": "bash scripts/deploy.sh",
    "start": "nuxt start",
    "generate": "nuxt generate -c nuxt.config.prod.js",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push --tags && npm run release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "conventional-github-releaser -p angular && npm run deploy && npm run dev",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "element-ui": "^2.4.11",
    "js-yaml": "^3.13.1",
    "json-query": "^2.2.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-sass": "^4.14.1",
    "nuxt": "^1.4.4",
    "nuxt-i18n": "^6.13.11",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-vue": "^0.3.3",
    "style-loader": "^0.23.1",
    "vue-awesome": "^3.2.0",
    "vue-style-loader": "^4.1.2",
    "vue-tables-2": "^1.6.25",
    "vue-tour": "^1.5.0",
    "vuex-persistedstate": "^3.1.0",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
